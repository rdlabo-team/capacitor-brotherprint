{
  "name": "@rdlabo/capacitor-brotherprint",
  "version": "0.0.4",
  "description": "Capacitor plugin for Brother Print SDK",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "npm-publish": "np"
  },
  "author": "Masahiko Sakakibara<sakakibara@rdlabo.jp>",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^2.4.0",
    "@capacitor/core": "^2.4.0",
    "@capacitor/ios": "^2.4.0",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "np": "^6.3.2",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "~3.8.3",
    "husky": "^4.2.5",
    "pretty-quick": "^2.0.1"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "RdlaboCapacitorBrotherprint.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:rdlabo-team/capacitor-brotherprint.git"
  },
  "bugs": {
    "url": "git@github.com:rdlabo-team/capacitor-brotherprint.git/issues"
  }
}
